{
    "i18n-ally.localesPaths": ["src/locales"],// 翻译文件路径 (自动生成) 相对于项目根目录的语言环境目录路径
    "i18n-ally.keystyle": "flat", // 翻译路径格式 (翻译后变量格式 nested：嵌套式  flat:扁平式)
    "i18n-ally.sourceLanguage": "en", // 翻译源语言 (源文件) 根据此语言文件翻译其他语言文件的变量和内容
    "i18n-ally.displayLanguage": "en", // 显示语言 (显示文件/翻译文件)
    "editor.formatOnSave": true 
}